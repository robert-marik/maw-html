echo "Updating Catalan .mo file"
cd ./locale
msgfmt ca_ES.po --output-file ca_ES.mo

echo "Updating Czech .mo file"
msgfmt cs_CZ.po --output-file cs_CZ.mo

echo "Updating Polish .mo file"
msgfmt pl_PL.po --output-file pl_PL.mo

echo "Updating English .mo file"
msgfmt en_US.po  --output-file en_US.mo

echo "Updating French .mo file"
msgfmt fr_FR.po  --output-file fr_FR.mo

echo "Updating Chinese .mo file"
msgfmt zh_CN.po  --output-file zh_CN.mo

echo "Updating Russian .mo file"
msgfmt ru_RU.po  --output-file ru_RU.mo

cd -